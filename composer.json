{
    "name": "brixion/bardecoder",
    "description": "GS1 HIBC barcode decoder",
    "keywords": [
        "GS1",
        "HIBC",
        "UDI",
        "barcode",
        "decoder"
    ],
    "homepage": "https://github.com/Brixion/Bardecoder",
    "type": "library",
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.88",
        "phpunit/phpunit": "^9.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Brixion\\Bardecoder\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brixion\\Bardecoder\\Test\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Maarten Olijve",
            "email": "maarten@brixion.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "testall": "phpunit tests",
        "testprimary": "phpunit tests/HbicLicPrimaryDataStructureTest.php",
        "testsecondary": "phpunit tests/HbicLicSecondaryDataStructureTest.php"
    }
}
